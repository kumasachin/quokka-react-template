{
  "name": "cyber-smart",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "turbo run dev:vite",
    "build": "turbo run build:vite",
    "lint": "turbo run lint:eslint",
    "typecheck": "turbo run typecheck:tsc",
    "test": "turbo run test:vitest",
    "preview": "turbo run preview:vite",
    "dev:vite": "vite",
    "build:vite": "tsc && vite build",
    "lint:eslint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck:tsc": "tsc --noEmit",
    "test:vitest": "echo \"No tests specified\" && exit 0",
    "preview:vite": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.2",
    "@types/styled-components": "^5.1.34",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@types/node": "^24.4.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "turbo": "^1.13.4",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
